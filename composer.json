{
    "name": "aces/archimedes-pipelines",
    "description": "ARCHIMEDES Data Ingestion Pipelines for LORIS",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aces/loris-php-api-client"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "loris/php-client": "dev-main",
        "guzzlehttp/guzzle": "^7.8",
        "monolog/monolog": "^3.5",
        "phpmailer/phpmailer": "^6.9"
    },
    "autoload": {
        "psr-4": {
            "LORIS\\": "src/"
        }
    }
}